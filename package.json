		"@vcarl/remark-headings": "^0.0.1",
		"autoprefixer": "^10.4.7",
		"cssnano": "^5.0.15",
		"eslint": "8.56.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte": "^2.35.1",
		"gh-pages": "^6.1.1",
		"mdsvex": "^0.11.0",
		"mdsvex-relative-images": "^1.0.3",
		"postcss": "^8.4.14",
